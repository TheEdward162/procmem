{
	"compilerOptions": {
		"strict": true,
		"rootDir": "src/",
		"outDir": "dist/",
		"module": "ESNext",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"declaration": false,
		"target": "ES2020",
		"jsx": "react",
		"jsxFactory": "window.SP_REACT.createElement",
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true
	},
	"include": [
		"src/"
	]
}
